{
    "name": "wine-app",
    "version": "1.0.0",
    "description": "Wine trip SPA",
    "main": "index.js",
    "repository": {},
    "scripts": {
        "clean": "rimraf dist",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "start": "node index.js",
        "start:prod": "cross-env NODE_ENV=production node index.js",
        "type-check-client": "tsc -p ./src/client/tsconfig.json",
        "build-client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode production",
        "build-server": "tsc -b src/server",
        "dev-client": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack watch",
        "dev-server": "tsc-watch --noClear -b ./src/server/tsconfig.json --onSuccess \"node --enable-source-maps index.js\"",
        "dev": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
        "build": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\""
    },
    "author": "Daniel Amsel",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.16.5",
        "@babel/plugin-proposal-class-properties": "^7.16.5",
        "@babel/plugin-proposal-decorators": "^7.16.5",
        "@babel/plugin-proposal-numeric-separator": "^7.16.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.16.5",
        "@babel/plugin-transform-runtime": "^7.16.5",
        "@babel/preset-env": "^7.16.5",
        "@babel/preset-react": "^7.16.5",
        "@babel/preset-typescript": "^7.16.5",
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@nestjs/testing": "^8.3.1",
        "@types/cssnano": "^5.0.0",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.0",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.2",
        "@types/webpack": "^4.41.26",
        "@types/webpack-bundle-analyzer": "^3.9.0",
        "@types/webpack-dev-server": "^3.11.1",
        "@types/webpack-manifest-plugin": "^3.0.3",
        "@types/workbox-webpack-plugin": "^5.1.8",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "babel-loader": "^8.2.3",
        "concurrently": "^6.5.0",
        "core-js": "^3.20.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.5.1",
        "cssnano": "^5.0.13",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "file-loader": "^6.2.0",
        "http-proxy-middleware": "^2.0.1",
        "postcss-loader": "^6.2.1",
        "prettier": "^2.5.1",
        "react-router": "^6.2.1",
        "react-router-dom": "^6.2.1",
        "request": "^2.88.2",
        "rimraf": "^3.0.2",
        "socket.io-client": "^4.4.1",
        "style-loader": "^3.3.1",
        "ts-node": "^10.4.0",
        "tsc-watch": "^4.5.0",
        "tsconfig-paths": "^3.12.0",
        "tslib": "^2.3.1",
        "typescript": "^4.5.5",
        "url-loader": "^4.1.1",
        "webpack": "^5.18.0",
        "webpack-bundle-analyzer": "^4.4.0",
